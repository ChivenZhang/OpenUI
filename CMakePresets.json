{
  "version": 6,
  "configurePresets": [
    {
      "name": "x64-Debug",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/Output/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_TOOLCHAIN_FILE": "$env{VCPKG_ROOT_CUSTOM}/scripts/buildsystems/vcpkg.cmake",
        "OPENUI_ENABLE_SAMPLE": "ON",
        "OPENUI_ENABLE_DIRECTX": "ON"
      }
    },
    {
      "name": "x64-Release",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/Output/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "RelWithDebInfo",
        "CMAKE_TOOLCHAIN_FILE": "$env{VCPKG_ROOT_CUSTOM}/scripts/buildsystems/vcpkg.cmake",
        "OPENUI_ENABLE_SAMPLE": "ON",
        "OPENUI_ENABLE_DIRECTX": "ON"
      }
    },
    {
      "name": "x64-osx-Debug",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/Output/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_TOOLCHAIN_FILE": "$env{VCPKG_ROOT_CUSTOM}/scripts/buildsystems/vcpkg.cmake",
        "OPENUI_ENABLE_SAMPLE": "ON",
        "OPENUI_ENABLE_METAL": "ON"
      }
    },
    {
      "name": "x64-osx-Release",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/Output/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "RelWithDebInfo",
        "CMAKE_TOOLCHAIN_FILE": "$env{VCPKG_ROOT_CUSTOM}/scripts/buildsystems/vcpkg.cmake",
        "OPENUI_ENABLE_SAMPLE": "ON",
        "OPENUI_ENABLE_METAL": "ON"
      }
    }
  ]
}